<robot name="dog">
  <link name="body">
   <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.0585" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.5 1 0.25"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.5 1 0.25"/>
      </geometry>
    </collision>
  </link>

  <link name="front_left_leg">
      <inertial>
        <mass value="0.01"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
      </inertial>
      <visual>
        <geometry>
          <sphere radius="0.0625"/>
        </geometry>
        <material name="Aqua"/>
      </visual>
      <collision>
        <geometry>
          <sphere radius="0.0625"/>
        </geometry>
      </collision>
  </link>


  <joint name="base_to_right_leg" type="continuous">
    <parent link="body"/>
    <child link="front_left_leg"/>
    <origin xyz="-0.125 -0.4375 -0.1875"/>
    <axis xyz="0 1 0" />
  </joint>

  <gazebo reference="ball">
    <material>Gazebo/Yellow</material>
  </gazebo>
</robot>
