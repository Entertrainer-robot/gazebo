<robot name="dog">
<!-- Define the body -->
  <link name="body">
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.0585" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.3 0.15 0.1"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.3 0.15 0.1"/>
      </geometry>
    </collision>
  </link>
  <!-- Connection the Body to the first leg-->
  <joint name="front_joint" type="fixed">
      <parent link="body"/>
      <child link="front_left_leg"/>
      <origin xyz="0.16 0.0 -0.1"/>
    </joint>
    <!-- Front Left Leg -->
    <link name="front_left_leg">
      <inertial>
        <mass value="0.01"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
      </inertial>
      <visual>
        <geometry>
          <box size="0.015 0.15 0.005"/>
        </geometry>
        <material name="Aqua"/>
      </visual>
      <collision>
        <geometry>
          <box size="0.015 0.15 0.005"/>
        </geometry>
      </collision>
    </link>




  <gazebo reference="tennis_ball">
    <material>Gazebo/Yellow</material>
  </gazebo>
</robot>
